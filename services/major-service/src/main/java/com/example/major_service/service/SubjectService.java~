package com.example.major_service.service;

import com.example.major_service.repository.SubjectRepository;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service
public class SubjectService {

    private final SubjectRepository userRepository;

    @Autowired
    public SubjectService(SubjectRepository userRepository) {
        this.userRepository = userRepository;
    }

}